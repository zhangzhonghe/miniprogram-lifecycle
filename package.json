{
  "name": "miniprogram-setup",
  "type": "module",
  "version": "0.0.7",
  "packageManager": "pnpm@7.2.1",
  "description": "小程序支持 setup 选项",
  "author": "张中合 <958414905@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/antfu/miniprogram-setup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/miniprogram-setup.git"
  },
  "bugs": "https://github.com/antfu/miniprogram-setup/issues",
  "keywords": [
    "小程序",
    "生命周期",
    "miniprogram",
    "lifecycle"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.17.2",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.6.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.20.0",
    "esno": "^0.16.3",
    "jsdom": "^20.0.0",
    "miniprogram-api-typings": "^3.5.0",
    "miniprogram-simulate": "^1.5.7",
    "pnpm": "^7.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6",
    "vite": "^3.0.4",
    "vitest": "^0.19.1"
  }
}
